if (typeof(MNGiRegistrationLoginUrl) == "undefined"){var MNGiRegistrationLoginUrl= "";}
if (typeof(MNGiRegistrationEmail) == "undefined"){var MNGiRegistrationEmail= "";}
if (MNGiRegistrationLoginStatus == "in") {document.write("<a name='user'>Welcome,&nbsp;" + MNGiRegistrationUserName + "</a><div class='navsep'>|</div><a href='" + MNGiRegistrationLoginUrl +"?rEmail=" + MNGiRegistrationEmail + "'>My Account</a><div class='navsep'>|</div><a href='" + MNGiRegistrationUrl + "/portlet/registration/html/logout.jsp'>Sign Out</a><div class='navsep'>|</div>");}
else {document.write("<a href='" + MNGiRegistrationLoginUrl + "'>Sign In</a><div class='navsep'>|</div><a href='" + MNGiRegistrationUrl + "/registration?rPage=register'>Register</a><div class='navsep'>|</div>");}
document.write("<a href='" + MNGiRegistrationLoginUrl + "?rEmail=" + MNGiRegistrationEmail + "#NL'>Newsletters</a><br>");
</script>

<!--END REGISTRATION-->
			</div>
	</div>
	
