<script>
   var tags= document.getElementsByTagName("DIV");
   for(x=0; x<tags.length; x++)
   {
   	<!-- ######### Removes Sorry no items ######### -->
      if(tags[x].className=="noListingItemTitle" && tags[x].innerHTML.indexOf("Sorry, no items are currently available.") != -1)
      {
         var clb = tags[x].parentNode;
         //Traverse the DOM tree backwards until we hit a table with a class name of "complexListingBox"
         while(clb.className != "complexListingBox")
         {
            clb = clb.parentNode;
         }
         clb.style.display="none";
      }

		<!-- ######### Add the Re-print icon ######### -->
		if(tags[x].className=="articleOptions")
		{
			//alert(tags[x].style.display);
			if(tags[x].innerHTML.indexOf("href")> -1)
			{
				aTags = tags[x].getElementsByTagName("A");
				if(aTags.length > 2)
				{
					spanTag = document.createElement("SPAN");

					tagA = '<a href=\'javascript: popup("RePrint","http://reprints.ygsgroup.com/m/bayareanews",1000,800);\' class="articleOptions" style="margin-left: 10px"><img src="http://extras.mnginteractive.com/live/media/site568/2007/0104/20070104_055350_button_reprint_license_this.gif" border="0" height="16" width="16"></a>';
					tagB = '<a href=\'javascript: popup("RePrint","http://reprints.ygsgroup.com/m/bayareanews",1000,800);\' class="articleOptions" style="margin-left: 2px; margin-right: 10px">Reprint</a>';

					spanTag.innerHTML = "".concat(tagA.concat(tagB));
					tags[x].insertBefore(spanTag, aTags[0]);
				}
			}
		}

		<!-- ######### Alter the anchor tags for Post a Comment links ######### -->
		if(tags[x].className=="post_balloon")
		{
			pnode = tags[x].parentNode;

			if(pnode.className=="listingArticleLinkTo")
				pnode.href=pnode.href+"#recent_comm";
		}

   }
   
	<!-- ### Hide the comments box when the article has expired ### -->
	if(document.getElementById("expiredArticleMsgId")!=null && document.getElementById("articleComments")!=null)
		document.getElementById("articleComments").style.display="none";  
</script>


