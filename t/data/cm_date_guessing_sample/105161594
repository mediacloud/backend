<div id="most-popular" class="boxwhite complexListingBox" style="margin-left:20px; margin-top:10px; clear:both; min-width:300px; width:500px;">
<script type="text/javascript" src="http://extras.denverpost.com/cache/popular/mercurynews_pop_lookup.js"></script>
<script type="text/javascript">
// The Most-Popular addon puts a little blurb of text 
// at the bottom of an article that lets readers know
// where this article is in the most-pop rankings,
// and directs them to other most-pop content.
//
// To do this, we look up the article id against an array defined in http://extras.denverpost.com/cache/popular/PROPERTYDOMAINBEFORETHEDOTCOM_pop_lookup.js
// ex: http://extras.denverpost.com/cache/popular/mercurynews_pop_lookup.js

// Parse the article_id out from the URL
var regex = /ci_([0-9]+)/i;
var result = regex.exec(document.location);
article_id = 0;
if ( result.length > 0 ) article_id = result[1];

function to_ordinal(num)
{
    var n = num % 100;
    var suff = ["th", "st", "nd", "rd", "th"]; // suff for suffix
    var ord= n<21?(n<4 ? suff[n]:suff[0]): (n%10>4 ? suff[0] : suff[n%10]);
    if ( num == 1 ) return '<strong>#1</strong>';
    return num + ord;
}


var pop_section_lookup = isNaN(pop_section[article_id]);
var pop_full_lookup = isNaN(pop_full[article_id]);
var pop_hour_lookup = isNaN(pop_hour[article_id]);
var pop_sansstate_lookup = isNaN(pop_sansstate[article_id]);
var pop_sansstate_hour_lookup = isNaN(pop_sansstate_hour[article_id]);
var pop_state_lookup = isNaN(pop_state[article_id]);
var pop_state_hour_lookup = isNaN(pop_state_hour[article_id]);
var pop_city_lookup = isNaN(pop_city[article_id]);

var pop_flag = false;
// If we have a number on the section-based lookup or the full lookup or the hour lookup,
// we know we've got something to publish.
//
// The challenge here is creating a legible sentence with the six possible combinations.
if ( pop_section_lookup == false || pop_full_lookup == false || pop_hour_lookup == false ) 
{
    document.write('<h4 class="articleSubHead" style="margin-bottom:0;">This article is the:</h4>');
    if ( pop_hour_lookup == false )
    {
        document.write('<li class="listNoImage"><a class="complexListingLink" href="/popular#popularhour">' + to_ordinal(pop_hour[article_id]) + ' most-clicked article of the hour</a></li>');
    }
    if ( pop_full_lookup == false )
    {
        document.write('<li class="listNoImage"><a class="complexListingLink" href="/popular">' + to_ordinal(pop_full[article_id]) + ' most-clicked article of the day</a></li>');
    }
    if ( pop_state_lookup == false )
    {
        document.write('<li class="listNoImage"><a class="complexListingLink" href="/popular#popularstate">' + to_ordinal(pop_state[article_id]) + ' most-clicked article among *STATENAME* readers of the day</a></li>');
    }
    if ( pop_state_hour_lookup == false )
    {
        document.write('<li class="listNoImage"><a class="complexListingLink" href="/popular#popularstate">' + to_ordinal(pop_state_hour[article_id]) + ' most-clicked article among *STATENAME* readers of the hour</a></li>');
    }
    if ( pop_sansstate_lookup == false )
    {
        document.write('<li class="listNoImage"><a class="complexListingLink" href="/popular">' + to_ordinal(pop_sansstate[article_id]) + ' most-clicked article among readers outside *STATENAME* of the day</a></li>');
    }
    if ( pop_sansstate_hour_lookup == false )
    {
        document.write('<li class="listNoImage"><a class="complexListingLink" href="/popular">' + to_ordinal(pop_sansstate_hour[article_id]) + ' most-clicked article among readers outside *STATENAME* of the hour</a></li>');
    }
    if ( pop_city_lookup == false )
    {
        document.write('<li class="listNoImage"><a class="complexListingLink" href="/popular#popularcity">' + to_ordinal(pop_state[article_id]) + ' most-clicked article among *CITYNAME* readers</a></li>');
    }
    if ( pop_section_lookup == false ) 
    {
        document.write('<li class="listNoImage"><a class="complexListingLink" href="/popular#pop-' + pop_section_name[article_id] + '">' + to_ordinal(pop_section[article_id]) + ' most-clicked article among ' + pop_section_name[article_id] + ' articles today</a></li>');
    }
    
    document.write('<li class="listNoImage"><em><a class="complexListingLink" href="#top">Back to the top</a></em></li>');
    pop_flag = true;
}
</script>
</div>
