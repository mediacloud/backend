<script type="text/javascript">
	//Get the article id
	if ( typeof(articleId) != 'undefined' && articleId > 1 ) 
		var article_id = articleId;
	// Redirect to 404 page 
	if ( document.getElementById('expiredArticleMsgId')) top.location.replace
	('http://www.mercurynews.com/404/ci_20040400?source=404_' + article_id);
