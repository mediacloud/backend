    <div class="asset-header">
			<script type="text/javascript">
			if (document.getElementById('gigyaShareBar_top') === null) {
				document.write('<div id="gigyaShareBar_top"></div>');
				Sacbee.load([ 'API', 'Ext.Gigya', 'Shortener' ], function() {
						var api = new Sacbee.API('f1ZXnae54o8tYnTMvIsstTgFz0EngrWUAosmBr3nTS5I0w2ZzygsIx9k7DeM5bEx');
						Sacbee.Shortener.shorten('http://blogs.sacbee.com/capitolalertlatest/2012/08/at-hotel-service-california-mormon-delegates-consider-romneys-personal-side.html', function(evt) {
								var lurl = evt.longURL;
								var surl = (evt.success) ? evt.shortURL : lurl;
								var act = new gigya.socialize.UserAction();
								act.setTitle('Sacbee - At hotel service, California\'s Mormon delegates consider Romney\'s personal side');
								act.setLinkBack(surl);
								act.setDescription('ST. PETE BEACH, Fla. - In a week in which Mitt Romney is expected to highlight his personal side - including, perhaps, his Mormon faith - Harold Stuart, an organist and alternate delegate to the Republican National Convention, accompanied California...');
								//act.addActionLink('View this post', this.state.gallery.shortURL);
								//act.addMediaItem({ type: 'image', src: this.state.photo.imagesByLabel.embedded.url, href: this.state.gallery.shortURL });
								var params = {
										containerID: 'gigyaShareBar_top',
										shareButtons: [
												{ provider: 'Share'/*, iconImgUp: 'https://cdns.gigya.com/gs/i/sharebar/icons/share3.png' */ },
												{ provider: 'Facebook' },
												{ provider: 'googleplus-share' },
												{ provider: 'Stumbleupon', url: lurl },
												{ provider: 'Twitter' },
												{ provider: 'Email' }
											],
										showCounts: 'none',
										shortURLs: (evt.success) ? 'never' : 'whenRequired',
										userAction: act
									};
								gigya.socialize.showShareBarUI(params);
							}, 'sb-bitly', { 'api': api });
					});
			}
