				Comments.addCommentMy(entry_id, last, 0);
			}
			setTimeout("CommentPoller.check(1979841);", 30000);
		}
	}
	CommentPoller.check(1979841);
	/* ShareBox.ad("ad_email"); */
</script>                </div>

