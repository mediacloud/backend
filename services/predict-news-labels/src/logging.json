{
  "version": 1,
  "disable_existing_loggers": true,

  "formatters": {
    "generic": {
      "format": "[%(asctime)s][%(levelname)s] %(name)s %(filename)s:%(funcName)s:%(lineno)d | %(message)s",
      "datefmt": "%H:%M:%S"
    }
  },

  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "generic",
      "stream": "ext://sys.stdout"
    }
  },

  "loggers": {
    "": {
      "handlers": ["console"],
      "level": "DEBUG",
      "propagate": true
    }
  }
}
